<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.kesdip.business.domain.admin.generated.Parameter"
		table="PARAMETER" lazy="true">
		<meta attribute="class-description">
			Domain object for the 'Parameter' entity. Auto-generated code.
			&lt;strong&gt;Do not modify manually.&lt;/strong&gt; @author
			gerogias
		</meta>
		<meta attribute="implement-equals">true</meta>

		<id name="id" column="ID" type="java.lang.Long">
			<meta attribute="use-in-equals">true</meta>
			<meta attribute="use-in-tostring">true</meta>
			<meta attribute="field-description">
				Primary, surrogate key.
			</meta>
			<generator class="increment" />
		</id>

		<property name="name" column="NAME" type="java.lang.String"
			length="512" not-null="true">
			<meta attribute="use-in-equals">true</meta>
			<meta attribute="use-in-tostring">true</meta>
			<meta attribute="field-description">
				The name of the parameter.
			</meta>
		</property>

		<property name="value" column="PARAM_VALUE" type="java.lang.String"
			length="512" not-null="true">
			<meta attribute="use-in-tostring">true</meta>
			<meta attribute="field-description">
				The value of the parameter.
			</meta>
		</property>

	</class>

</hibernate-mapping>
